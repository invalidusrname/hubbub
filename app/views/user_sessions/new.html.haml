- content_for :title, 'Login'

- form_for @user_session, :url => user_session_path do |f|
  = f.error_messages
  
  .required
    = f.label :email
    = f.text_field :email
  .required
    = f.label :password
    = f.password_field :password
  %div
    = f.label :remember_me
    = f.check_box :remember_me
  %div
    = f.submit 'Login'